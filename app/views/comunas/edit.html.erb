<%= render 'users/shared/up'%>

<h1>Editar Comuna</h1>

<p> <%= notice %> </p>

<%= form_with(model: @comuna, method: :patch) do |form| %>

    <div class="field">
        <%=form.label :nombre%>
        <%=form.text_field :nombre%>
    </div>
    
    <div class="action">
        <%=form.submit%>
    </div>
    
<% end %>